{
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.probe": ["javascript", "typescript", "markdown"],
  "jest.runMode": {
    "type": "on-demand"
  }
}
